# API Keys (at least one LLM required)
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_gemini_api_key_here

# External API Keys for Discovery

# Foursquare Places API (FREE - 50 requests/day, no credit card required)
# Get key at: https://foursquare.com/developers/signup
FOURSQUARE_API_KEY=your_foursquare_api_key_here

# Note: If no Foursquare key provided, app will automatically use 
# OpenStreetMap API (completely free, no API key needed)

# TMDb API (FREE - 1000 requests/day)
TMDB_API_KEY=your_tmdb_api_key_here

# Optional APIs
EVENTBRITE_API_KEY=your_eventbrite_api_key_here
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Model Configuration (optional)
LLM_MODEL=gemini/gemini-1.5-flash
TEMPERATURE=0.7
